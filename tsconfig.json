{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "sourceMap": false,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node"
  },
  "include": ["src", "types"]
}
